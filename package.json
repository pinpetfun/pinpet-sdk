{
  "name": "spin-sdk",
  "version": "0.1.0",
  "description": "SpinPet protocol SDK for Solana - A comprehensive toolkit for interacting with SpinPet smart contracts",
  "main": "dist/spin-sdk.cjs.js",
  "module": "dist/spin-sdk.esm.js",
  "browser": "dist/spin-sdk.js",
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c -w",
    "build:prod": "NODE_ENV=production rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build:prod && npm run lint",
    "test": "mocha tests/unit/**/*.test.js",
    "test:integration": "mocha tests/integration/**/*.test.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "size-check": "npm run build:prod && du -sh dist/*"
  },
  "keywords": [
    "solana",
    "anchor",
    "blockchain",
    "sdk",
    "defi",
    "trading",
    "spinpet"
  ],
  "author": "SpinPet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spinpet/spin-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/spinpet/spin-sdk/issues"
  },
  "homepage": "https://github.com/spinpet/spin-sdk#readme",
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@pump-fun/pump-sdk": "^1.5.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.78.0",
    "axios": "^1.11.0",
    "bs58": "^4.0.1",
    "decimal.js": "^10.6.0",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.0.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ]
}
